<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="edu.umd.mindlab.androidservicetest.SendDeviceInfo"
    tools:showIn="@layout/activity_send_device_info">

    <TextView
        android:id="@+id/fakeTerms"
        android:layout_width="211dp"
        android:layout_height="114dp"
        android:text="Terms and Conditions"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="71dp"
        android:layout_marginTop="107dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="71dp" />

    <Button
        android:id="@+id/backToMain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Main"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/fakeTerms"
        android:layout_marginTop="74dp"
        app:layout_constraintTop_toBottomOf="@+id/fakeTerms"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/fakeTerms" />
</android.support.constraint.ConstraintLayout>
