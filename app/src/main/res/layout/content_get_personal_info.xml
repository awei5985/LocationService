<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="edu.umd.mindlab.androidservicetest.GetPersonalInfo"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp"
    tools:showIn="@layout/activity_get_personal_info">

    <EditText
        android:id="@+id/firstNameEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="24dp"
        android:ems="10"
        android:hint="First Name"
        android:inputType="textPersonName|textNoSuggestions"
        android:nextFocusDown="@+id/lastNameEdit"
        app:layout_constraintRight_toLeftOf="@+id/lastNameEdit"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/lastNameEdit"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/lastNameEdit"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="20dp" />

    <EditText
        android:id="@+id/lastNameEdit"
        android:layout_width="150dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="textPersonName|textNoSuggestions"
        android:layout_marginEnd="40dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/dobEdit"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="94dp"
        android:layout_marginBottom="94dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="40dp" />

    <EditText
        android:id="@+id/dobEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="e.g. 01/01/00"
        android:inputType="date"
        android:layout_marginStart="15dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/birthCityEdit"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="43dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView3"
        android:layout_marginLeft="15dp" />

    <EditText
        android:id="@+id/birthCityEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="e.g. College Park"
        android:inputType="textPersonName|textNoSuggestions"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/uidEdit"
        app:layout_constraintRight_toRightOf="@+id/dobEdit"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="45dp"
        app:layout_constraintLeft_toLeftOf="@+id/dobEdit" />

    <EditText
        android:id="@+id/uidEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="e.g. 123456789"
        android:inputType="number"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/birthCityEdit"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/birthCityEdit"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="241dp"
        android:layout_height="39dp"
        android:layout_marginBottom="44dp"
        android:layout_marginLeft="-8dp"
        android:layout_marginTop="55dp"
        android:text="Name (enter exactly as registered with University)"
        app:layout_constraintBottom_toBottomOf="@+id/firstNameEdit"
        app:layout_constraintLeft_toLeftOf="@+id/firstNameEdit"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="Date of Birth (mm/dd/yyyy)"
        app:layout_constraintBottom_toTopOf="@+id/dobEdit"
        app:layout_constraintLeft_toLeftOf="@+id/firstNameEdit"
        app:layout_constraintRight_toLeftOf="@+id/lastNameEdit"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="0dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="87dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="87dp"
        android:text="Birth City"
        app:layout_constraintBottom_toTopOf="@+id/uidEdit"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="147dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:text="UID"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/submitPersInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit Info"
        android:layout_marginEnd="15dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/uidEdit"
        android:layout_marginBottom="32dp"
        android:layout_marginRight="15dp" />

</android.support.constraint.ConstraintLayout>

